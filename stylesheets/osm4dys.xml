    <Style name="flags">
        <Rule>
            &maxscale_zoom4;
            &minscale_zoom4;
            <Filter>[labelrank] &lt;= 3 and ([type] = 'Sovereign country' or [type] = 'Country') and [pop_est] &gt; 1000000 </Filter>
            <PointSymbolizer file="[flag_file]" allow-overlap="no" />
        </Rule>
        <Rule>
            &maxscale_zoom5;
            &minscale_zoom5;
            <Filter>([type] = 'Sovereign country' or [type] = 'Country') and [pop_est] &gt; 1000000 </Filter>
            <PointSymbolizer file="[flag_file]" allow-overlap="no" />
        </Rule>
    </Style>    
    <Layer name="flags" status="on" srs="&srs_3857;">
        <StyleName>flags</StyleName>
        <Datasource>
            <Parameter name="table">
            <![CDATA[
                (
                SELECT centroid, name, 'national_flags_svg/Flag_of_' || replace(name,' ','_')  || '.png' as flag_file, labelrank, type, pop_est
                FROM constant_data.ne_50m_admin_0_countries
                ) AS flags
            ]]>            
            </Parameter>
            <Parameter name="geometry_field">centroid</Parameter>
            <Parameter name="type">&db_type;</Parameter>
            <Parameter name="password">&db_password;</Parameter>
            <Parameter name="host">&db_host;</Parameter>
            <Parameter name="port">&db_port;</Parameter>
